	'MainForm.RootPane.LoadLayout("Layout1") 'Load the layout file.
	MainForm.Show
	
	driver.Initialize("d://chromedriver.exe")
	driver.Get("http://162.105.134.188/pdfindex.jsp?fid=c54fedd7ab7901cbacd09d5f4fdac124&xyz=0.7909188656409809")
