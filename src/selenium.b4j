Sub searchYCY
	driver.Get("http://www.baidu.com")
	Log(driver.findElement(By.id("kw")))
	Dim kw As WebElement=driver.findElement(By.id("kw"))
	Log(kw.getAttribute("name"))
	kw.click
	kw.sendKeys("杨超越")
	Dim submit As WebElement=driver.findElement(By.id("su"))
	submit.click
End Sub

